FileDaemon {
  Name = "<%= @fqdn %>-fd"
  FDPort = <%= @port %>
  Working Directory = "<%= @working_directory %>"
  Pid Directory = "<%= @pid_directory %>"
  Maximum Concurrent Jobs = <%= @max_concurrent_jobs %>

  TLS Enable = yes
  TLS Require = yes
  TLS CA Certificate File = "<%= @tls_ca %>"
  TLS Certificate = "<%= @tls_cert %>"
  TLS Key = "<%= @tls_key %>"
}
